
{
    "port":    "Blue-Pill",
    "board":   "Blue-Pill",
    "backend": "arduino",

    "cc_linux":   "arduino-cli",
    "cc_windows": "arduino-cli.exe",
    "cc_make_flags":	  "compile -b STMicroelectronics:stm32:Blues -v --output-dir @{file_dir_name}/build",
	"flash_command":	"upload @{file_dir_name} -p /dev/ttyUSB0 -b esp32:esp32:esp32",

    "api_paths": [
        "ST/Blue-Pill",
        "Arduino/arduino-generic",
        "Arduino/arduino-generic-UART"
    ],
    "v_defines": [
    ],

    "default_cpu_freq": 72000000,
    "default_string_len": 50,

    "compiler_setup_path": "setup/arduino32.json"
}
