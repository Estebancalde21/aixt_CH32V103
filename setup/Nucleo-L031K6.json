
{
    "port":    "Nucleo-L031K6",
    "board":   "Nucleo-L031K6",
    "backend": "arduino",

    "cc_linux":			"arduino-cli",
    "cc_windows":		"arduino-cli.exe",
    "cc_make_flags":	"compile -b STMicroelectronics:stm32:GenL0 -v --output-dir @{file_dir_name}/build",

    "flasher_linux":    "arduino-cli",
    "flasher_windows":  "arduino-cli.exe",
	"flasher_flags":	"upload @{file_dir_name} -p @{port} -b STMicroelectronics:stm32:GenL0",

    "api_paths": [
        "ST/Nucleo-L031K6",
        "Arduino/arduino-generic",
        "Arduino/arduino-generic-UART",
        "Arduino/arduino-generic-i2c"
    ],
    "v_defines": [
    ],
    
    "default_cpu_freq": 32000000,
    "default_string_len": 50,
    
    "compiler_setup_path": "setup/arduino32.json"
}