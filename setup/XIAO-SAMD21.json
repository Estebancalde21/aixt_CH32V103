{
    "port": "XIAO-SAMD21",
    "board": "Seeed Studio XIAO SAMD21",
    "backend": "arduino",

    "cc_linux":			"arduino-cli",
    "cc_windows":		"arduino-cli.exe",
    "cc_make_flags":	"compile -b arduino:samd:nano_33_iot -v --output-dir @{file_dir_name}/build",

    "flasher_linux":    "arduino-cli",
    "flasher_windows":  "arduino-cli.exe",
	"flasher_flags":	"upload @{file_dir_name} -p /dev/ttyUSB0 -b esp32:esp32:esp32",

    "api_paths": [
        "Seeed-Studio/XIAO-SAMD21",
        "Arduino/arduino-generic",
        "Arduino/arduino-generic-UART"
    ],

    "v_defines": [
    ],

    "default_cpu_freq": 48000000,
    "default_string_len": 50,

    "compiler_setup_path": "setup/arduino32.json"
}