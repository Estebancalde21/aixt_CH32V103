{
    "port": "XIAO-ESP32-xx",
    "board": "Seeed Studio XIAO ESP32-C3",
    "backend": "arduino",

    "cc_linux":			"arduino-cli",
    "cc_windows":		"arduino-cli.exe",
    "cc_make_flags":	"compile -b esp32:esp32:XIAO_ESP32C3 -v --output-dir @{file_dir_name}/build",

    "flasher_linux":    "arduino-cli",
    "flasher_windows":  "arduino-cli.exe",
	"flasher_flags":	"upload @{file_dir_name} -p @{port} -b esp32:esp32:XIAO_ESP32C3",

    "api_paths": [
        "Seeed-Studio/XIAO-ESP32-xx",
        "Arduino/arduino-generic",
        "Arduino/arduino-generic-UART"
    ],

    "v_defines": [
    ],

    "default_cpu_freq": 48000000,
    "default_string_len": 50,

    "compiler_setup_path": "setup/arduino32.json"
}