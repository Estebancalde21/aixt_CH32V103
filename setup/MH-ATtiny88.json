{
    "port": "MH-ATtiny88",
    "board": "MH-ATtiny88",
    "backend": "arduino",
    "cc_linux": "arduino-cli",
    "cc_windows": "arduino-cli.exe",
    "cc_make_flags": "compile -b --fqbn MH-ET.menu.avr.ATTiny88:avr:attiny88 -v --output-dir @{file_dir_name}/build",
    "flasher_linux": "arduino-cli",
    "flasher_windows": "arduino-cli.exe",
    "flasher_flags": "upload @{file_dir_name} -p @{port} -b --fqbn MH-ET.menu.avr.ATTiny88:avr:attiny88",
    "api_paths": [
        "Atmel/MH-ATtiny88",
        "Arduino/arduino-AVR",
        "Arduino/arduino-generic",
        "Arduino/arduino-generic-UART",
        "Arduino/arduino-generic-i2c"
    ],
    "v_defines": [],
    "default_cpu_freq": 16000000,
    "default_string_len": 50,
    "compiler_setup_path": "setup/arduino8.json"
}