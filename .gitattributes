# Mark V source files as V language
*.v linguist-language=v
*v.mod linguist-language=v
*.vsh linguist-language=v

# Mark C files generated by V as generated
*.c linguist-generated=true
*.h linguist-generated=true
*.ino linguist-generated=true
*.nxc linguist-generated=true

# ports/**/projects/** linguist-vendored=true
# ports/**/**/projects/** linguist-vendored=true
# ports/**/**/**/projects/** linguist-vendored=true
# ports/**/examples/** linguist-vendored=true
# ports/**/codegentemp/** linguist-vendored=true
#  ports/**/projects/*.X/** linguist-vendored=true
#  ports/**/projects/*.cydsn/** linguist-vendored=true
#  ports/**/projects/**/*.h linguist-vendored=true
#  ports/**/projects/**/*.c linguist-vendored=true
#  ports/**/**/projects/** linguist-vendored=true
#  ports/*/projects/*.X/** linguist-vendored=true

#  assets/** linguist-documentation=true

# others
*.i linguist-vendored
*.mk linguist-vendored
*.inc linguist-vendored
*.s linguist-vendored
*.bat linguist-vendored
*.P linguist-vendored
*.sdc linguist-vendored
*.ctl linguist-vendored
*.html linguist-vendored

*.ipynb linguist-vendored

# Ignore Makefiles in language statistics
*Makefile linguist-vendored
*makefile linguist-vendored

# Mark binary files as binary
*.d binary
*.o binary
*.hex binary
*.elf binary
*.bin binary

# Mark build directories as linguist-vendored
/build/ linguist-vendored
/bin/ linguist-vendored
/out/ linguist-vendored

# Mark dependency directories as linguist-vendored
.vmodules/ linguist-vendored