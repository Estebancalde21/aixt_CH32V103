[
  {
    "label": "Aixt: Transpile",
    "command": "aixt",
    "args": ["transpile", "Arduino-Mega", "$ZED_FILE"],
    "use_new_terminal": false,
    "allow_concurrent_runs": false,
    "reveal": "always"
  },
  {
    "label": "Aixt: Compile",
    "command": "aixt",
    "args": ["compile", "Arduino-Mega", "$ZED_FILE"],
    "use_new_terminal": false,
    "allow_concurrent_runs": false,
    "reveal": "always"
  },
  {
    "label": "Aixt: Build",
    "command": "aixt",
    "args": ["build", "Arduino-Mega", "$ZED_FILE"],
    "use_new_terminal": false,
    "allow_concurrent_runs": false,
    "reveal": "always"
  },
  {
    "label": "Aixt: Flash",
    "command": "aixt",
    "args": ["flash", "Arduino-Mega", "$ZED_FILE", "/dev/ttyUSB0"],
    "use_new_terminal": false,
    "allow_concurrent_runs": false,
    "reveal": "always"
  },
  {
    "label": "Aixt: Clean",
    "command": "aixt",
    "args": ["clean", "Arduino-Mega", "$ZED_FILE"],
    "use_new_terminal": false,
    "allow_concurrent_runs": false,
    "reveal": "always"
  }
]
